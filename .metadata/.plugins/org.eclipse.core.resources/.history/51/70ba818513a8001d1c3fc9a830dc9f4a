package midterm1Question1;

public class CustomerDriver {

	public static void main(String[] args) {
		Customer cust1 = new Customer(01, "Eli Anderson", "35423 Hoffman Drive, Lakeville MN 55109 USA", "651-111-1111");
		Customer cust2 = new Customer(02, "Kennedy Peterson", "54975 Ashley Drive, Minneatonka MN 55110 USA", "651-222-2222");
	
		System.out.println(cust1.toString());
		System.out.println(cust2.toString());

		Customer cust3 = new Customer();
		System.out.println(cust3.toString());

	
	
	
	
	}

}
